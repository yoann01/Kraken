            }
          ],
        "extDeps" : {},
        "code" : "dfgEntry {
  // Resize array to draw digits
  result.resize(inputs.size() - 1);
  offsetXfos.resize(inputs.size() - 1);
  
  Quat digitPlaneOri;
  Vec3 digitUpVec;
  Vec3 digitDir;
  for (Integer i=0; i<result.size(); i++) {
    // Set Digit Plane Ori by this and next input
    digitUpVec = inputs[i].ori.getYaxis();
    digitDir = (inputs[i + 1].tr - inputs[i].tr).unit();
    digitPlaneOri.setFromDirectionAndUpvector(digitDir, digitUpVec);
    
    // Align X axis down the length
    Quat alignXOri;
    alignXOri.setFromAxisAndAngle(Vec3(0, 1, 0), HALF_PI * -1);
    digitPlaneOri = digitPlaneOri * alignXOri;
    
    // Set result xfo
    result[i].sc = inputs[i].sc;
    result[i].ori = digitPlaneOri;
    result[i].tr = inputs[i].tr;
    
    
    // Align Z axis towards input Y axis to align shape
    Quat alignZOri;
    alignZOri.setFromAxisAndAngle(Vec3(1, 0, 0), HALF_PI * -1);
    
    // Set offsetXfos
    offsetXfos[i] = result[i];
    offsetXfos[i].sc.x = (inputs[i + 1].tr - inputs[i].tr).length();
    offsetXfos[i].ori = digitPlaneOri * alignZOri;
    offsetXfos[i].tr = result[i].transformVector(Vec3((inputs[i + 1].tr - inputs[i].tr).length() / 2, 0, 0));
  }  

}
"
