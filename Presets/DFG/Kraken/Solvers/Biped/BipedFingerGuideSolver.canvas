operator Math_Xfo_toMat44_task<<<index>>>(
  io Mat44[] result
, in Xfo[] this
) {
  result[index] = this[index < this.size() ? index : this.size() - 1].toMat44();
}

dfgEntry {
  if( this.size() == 0) return;
  Size task_size = this.size();
  result.resize(task_size);
  if(task_size < 10000 /* pex margin */) {
    for(Size index = 0; index < task_size; index++) {
      Math_Xfo_toMat44_task(index, result, this);
    }
  } else {
    Math_Xfo_toMat44_task<<<task_size>>>(result, this);
  }
}
