require Math;

struct KrakenStringAttribute : KrakenAttribute {
  String value;
};

/// \dfgPresetFolder Attributes
function KrakenStringAttribute(
  String name,
  String path,
  Boolean keyable,
  Boolean animatable,
  String value
) {
  this.name = name;
  this.path = path;
  this.keyable = keyable;
  this.animatable = animatable;
  this.value = value;
}

/// \dfgPresetFolder Attributes
function String KrakenStringAttribute.getStringValue() {
  return this.value;
}
